{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Score",
            "group": "test",
            "args": [
                "-e",
                "codeoceanR::rt_score()"
            ],
            "type": "process",
            "command": "C:\\usr\\R-4.1.3\\bin\\Rscript.exe",
            "options": {
                "cwd": "${fileDirname}"
            }
        },
        {
            "label": "Submit",
            "group": "none",
            "args": [
                "-e",
                "codeoceanR::rt_submit()"
            ],
            "type": "process",
            "command": "C:\\usr\\R-4.1.3\\bin\\Rscript.exe",
            "options": {
                "cwd": "${fileDirname}"
            }
        },
        {
            "label": "Launch R",
            "group": "none",
            "type": "process",
            "command": "${config:r.rterm.windows}",
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": []
        }
    ]
}